I,KAAK,SAAL,CAAe,GAAf,EAAmB,QAAQ,CAAC,IAAT,CAAc,WAAjC,CAAnB;AACA,MAAA,OAAO,CAAC,GAAR,CAAY,SAAZ,EAAuB,UAAvB,EAnB0F,CAqB1F;AACA;AACA;;AACA,UAAI,IAAG,GAAI,IAAG,GAAG,IAAN,IAAe,IAAI,IAA9B;AAEA,UAAI,MAAK,GAAI,CAAC,UAAS,GAAK,aAAa,CAAC,CAA7B,IAAmC,IAAhD;AACA,UAAI,MAAK,GAAI,CAAC,EAAd,CA3B0F,CA4B1F;AACA;;AACA,UAAI,CAAA,GAAK,KAAK,SAAQ,GAAK,aAAa,CAAC,CAA3B,GAAgC,MAAK,GAAI,IAA9C,CAAD,IAAyD,IAAG,GAAI,IAAhE,CAAR,CA9B0F,CAgC1F;;AACA,UAAI,IAAG,GAAI,CAAX;AACA,UAAI,IAAG,GAAK,aAAa,CAAC,CAA1B;AACA,UAAI,IAAG,GAAK,aAAa,CAAC,CAA1B,CAnC0F,CAqC1F;;AACA,UAAI,CAAC,MAAM,CAAC,qBAAZ,EAAmC;AAC/B,QAAA,MAAM,CAAC,qBAAP,GAA+B,UAAC,EAAD,EAAkB;AAAA,cAAb,CAAa,uEAAT,IAAS;AAC7C,UAAA,UAAU,CAAC,EAAD,EAAK,CAAL,CAAV;AACH,SAFD;AAGJ,OA1C0F,CA2C1F;;;AACA,UAAI,IAAG,GAAI,SAAP,IAAO,GAAM;AACb;AACA,YAAI,IAAG,IAAK,SAAZ,EAAuB;AACnB;AACA,UAAA,MAAI,CAAC,OAAL,GAAe,MAAI,CAAC,OAAL,CAAa,MAAb,CAAoB,UAAC,OAAD,EAAW;AAC1C,mBAAO,OAAO,CAAC,EAAR,KAAe,IAAI,CAAC,EAA3B;AACH,WAFc,CAAf;AAGA,UAAA,IAAI,CAAC,MAAL,GALmB,CAMnB;;AACA;AACJ,SAVa,CAWb;;;AACA,QAAA,IAAG,IAAK,MAAR;AACA,QAAA,IAAG,IAAM,CAAA,GAAI,IAAJ,GAAW,MAApB;AACA,QAAA,qBAAqB,CAAC,YAAM;AACxB,UAAA,IAAI,CAAC,KAAL,CAAW,GAAX,GAAiB,IAAI,CAAC,GAAL,CAAS,IAAT,EAAe,SAAf,IAA4B,IAA7C;AACA,UAAA,IAAI,CAAC,KAAL,CAAW,IAAX,GAAkB,IAAI,CAAC,GAAL,CAAS,IAAT,EAAe,UAAf,IAA6B,IAA/C;AACA,UAAA,IAAI;;AACJ,cAAI,KAAJ,EAAW;AACP,YAAA,IAAI,CAAC,KAAL,CAAW,SAAX,uBAAoC,EAAE,GAAE,GAAI,MAAR,CAApC,iBAA0D,EAAE,GAAE,IAAK,CAAA,GAAI,IAAJ,GAAW,MAAhB,CAAJ,CAA1D;AACA,YAAA,IAAI,CAAC,KAAL,CAAW,OAAX,GAAqB,CAAC,IAAI,GAAL,IAAY,GAAjC;AACJ;;AACA,UAAA,IAAI;AACP,SAToB,CAArB;AAUH,OAxBD;;AAyBA,MAAA,IAAI;AAEJ;AArHI,GAXG;AAkIX,EAAA,OAlIW,qBAkID;AAAA;;AACN,IAAA,QAAQ,CAAC,gBAAT,CAA0B,OAA1B,EAAkC,UAAC,CAAD,EAAK;AACnC,UAAI,QAAO,GAAI,CAAC,CAAC,GAAF,CAAM,WAAN,EAAf;AACA,MAAA,OAAO,CAAC,GAAR,CAAY,QAAZ,EAFmC,CAGnC;;AACA,WAAK,IAAI,CAAA,GAAI,CAAb,EAAgB,CAAA,GAAI,MAAI,CAAC,OAAL,CAAa,MAAjC,EAAyC,CAAC,EAA1C,EAA8C;AAC1C,QAAA,OAAO,CAAC,GAAR,CAAY,MAAI,CAAC,OAAL,CAAa,MAAzB;;AACA,YAAI,MAAI,CAAC,OAAL,CAAa,CAAb,EAAgB,QAAhB,KAA6B,QAAjC,EAA2C;AACvC,cAAG,CAAC,MAAI,CAAC,OAAL,CAAa,MAAjB,EAAyB;AACzB,cAAI,WAAU,GAAI,MAAI,CAAC,OAAL,CAAa,CAAb,EAAgB,EAAlC;AACA,cAAI,SAAQ,GAAI,QAAQ,CAAC,cAAT,CAAwB,WAAxB,CAAhB;AACA,UAAA,SAAS,CAAC,MAAV;;AACA,UAAA,MAAI,CAAC,OAAL,CAAa,MAAb,CAAoB,CAApB,EAAsB,CAAtB;;AACA,UAAA,OAAO,CAAC,GAAR,CAAY,MAAI,CAAC,OAAjB;AACA;AACJ;AACJ;AACH,KAhBD;AAiBH;AApJU,CAAf","sourcesContent":["<template>\r\n    <div id=\"hahaha\" @click=\"playGame()\" >点击一下出现一个抛物线盒子</div>\r\n    <!-- <div style=\"top: 100px;width: 200px;height: 200px;\">\r\n        <div class=\"five-pointed-star\"></div>\r\n    </div>     -->\r\n    <!-- <button @click=\"removeDiv\">点击此处删除存在画面的第一个div</button> -->\r\n</template>\r\n\r\n<script>\r\nimport {nanoid} from 'nanoid';\r\n\r\nexport default {\r\n    name: 'ParabolicBall',\r\n    data() {\r\n        return {\r\n            elementArray:['ghost','square'],\r\n            colorArray:['#2A7E98','#f5a63','#D91649','#33D1A0','#31027D'],\r\n            // 存放生成的div盒子的id和显示的键值，盒子消失，就把id从idArray里去除\r\n            idArray:[],\r\n            yourScore: 0,\r\n        }\r\n    },\r\n    methods:{\r\n        // 生成随机的字符串数组\r\n        makeid(length) {\r\n            let text = [];\r\n            var possible =\"ABCDEFGHIJKLMNOPQRSTUVWXYZ\";\r\n\r\n            for (let i = 0; i < length; i++)\r\n                text.push( possible.charAt(Math.floor(Math.random() * possible.length)));\r\n\r\n            return text;\r\n        },\r\n        // Demo阶段（不完善），点击一下出现一个盒子，完全版，应该是传入一个字符串数组按照一定的时差顺序依次出现\r\n        playGame() {\r\n            let lettersArray = this.makeid(1);\r\n            lettersArray.forEach((letter) => {\r\n                this.parabolicRun(letter)\r\n            })\r\n        },\r\n\r\n        removeDiv() {\r\n            if(!this.idArray.length) return;\r\n            let elementId = this.idArray.shift().id;\r\n            let element = document.getElementById(elementId);\r\n            element.remove();\r\n            // console.log(this.idArray)\r\n        },\r\n\r\n        getRandom (n, m) {\r\n            var num = Math.floor(Math.random() * (m - n + 1) + n)\r\n            return num\r\n        },\r\n        \r\n        parabolicRun(letter) {\r\n            // 无残影\r\n            // console.log(event.clientX,event.clientY)\r\n            // this.addAnimation({x: event.clientX, y:event.clientY},'', '', 500);\r\n            this.addAnimation(letter,{x: this.getRandom(50,500), y: this.getRandom(350,450)},'', '', 500);\r\n            // 有残影\r\n            // this.ghost({x: event.clientX, y:event.clientY}, '', '', 500);\r\n        },\r\n        ghost(startPosition, endPosition, time, num) {\r\n            for (let i = 0; i < num; i++) {\r\n                this.addAnimation(startPosition, endPosition, time, i / (num + 1), true);\r\n            }\r\n        },\r\n        // 生成普通的小球 传入 1.初始坐标 2.结束点坐标 3.动画执行时长\r\n        addAnimation(letter,startPosition = { x: 0, y: 0 }, endPosition, time, num, ghost = false){\r\n        let item = document.createElement('div');\r\n        !ghost ? item.className = this.elementArray[Math.floor(Math.random() * this.elementArray.length)] : item.className = 'ghost';\r\n        item.style.backgroundColor = this.colorArray[Math.floor(Math.random() * this.colorArray.length)];\r\n        // if(item.className !== 'five-pointed-star') { \r\n        //     item.style.backgroundColor = this.colorArray[Math.floor(Math.random() * this.colorArray.length)];\r\n        // }\r\n        item.id = nanoid();\r\n        // console.log(item.id);\r\n        item.style.top = startPosition.y + 'px';\r\n        item.style.left = startPosition.x + 'px';\r\n        document.body.appendChild(item);\r\n        item.textContent = letter;\r\n        this.idArray.push({id:item.id,keyValue:letter});\r\n        // console.log(this.idArray);\r\n\r\n        // 目的地\r\n        const targetTop = endPosition.y || document.body.offsetHeight ;\r\n        const targetLeft = this.getRandom(800,document.body.offsetWidth) ;\r\n        console.log(targetTop, targetLeft)\r\n\r\n        // 采用固定动画时长方式\r\n        // 前者为动画预计执行时间 (非真正看到的动画时间正相关)\r\n        // 后者为每次动画执行间隔 (以普通60Hz的显示器为示范，每次刷新的间隔为 1/60 s)\r\n        let Time = time /0.02 || (4 / 0.02);\r\n\r\n        let Xspeed = (targetLeft - (startPosition.x)) / Time;\r\n        let Yspeed = -10;\r\n        // 控制下坠速度\r\n        // 设置一个加速度A  已知 L = 0.5 * A * T * T + Y * T\r\n        let A = (2 * (targetTop - (startPosition.y) - Yspeed * Time)) / (Time * Time);\r\n\r\n        // 时间单位 用以配合加速度计算当前速度 speed = Yspeed + A * T\r\n        let loop = 0;\r\n        let nowX  = startPosition.x;\r\n        let nowY  = startPosition.y;\r\n\r\n        // 解决少数浏览器兼容性问题\r\n        if (!window.requestAnimationFrame) {\r\n            window.requestAnimationFrame = (fn, t = 16.6) => {\r\n                setTimeout(fn, t);\r\n            };\r\n        }\r\n        // 执行动画\r\n        let move = () => {\r\n            // 判断是否要停止\r\n            if (nowY >= targetTop) {\r\n                // 停止 隐藏 销毁 退出\r\n                this.idArray = this.idArray.filter((idArray)=>{\r\n                    return idArray.id !== item.id;\r\n                })\r\n                item.remove();\r\n                // console.log(this.idArray)\r\n                return;\r\n            }\r\n            // 根据当前速度 计算最新位置\r\n            nowX += Xspeed;\r\n            nowY += (A * loop + Yspeed);\r\n            requestAnimationFrame(() => {\r\n                item.style.top = Math.min(nowY, targetTop) + 'px';\r\n                item.style.left = Math.min(nowX, targetLeft) + 'px';\r\n                loop++;\r\n                if (ghost) {\r\n                    item.style.transform = `translate(${-(num * Xspeed)}px ,${-(num * (A * loop + Yspeed))}px)`;\r\n                    item.style.opacity = (1 - num) * 0.5;\r\n                }\r\n                move();\r\n            });\r\n        };\r\n        move();\r\n            \r\n        }\r\n    },\r\n    mounted() {\r\n        document.addEventListener('keyup',(e)=>{\r\n            let pressKey = e.key.toUpperCase();\r\n            console.log(pressKey);\r\n            // 删除第一个符合键值的盒子\r\n            for (let i = 0; i < this.idArray.length; i++) {\r\n                console.log(this.idArray.length)\r\n                if (this.idArray[i].keyValue === pressKey) {\r\n                    if(!this.idArray.length) return;\r\n                    let removeDivId = this.idArray[i].id\r\n                    let removeDiv = document.getElementById(removeDivId);\r\n                    removeDiv.remove();\r\n                    this.idArray.splice(i,1)\r\n                    console.log(this.idArray);\r\n                    break;\r\n                }\r\n            }\r\n        })\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\nbody {\r\n   margin:0;\r\n   /* background-color:black; */\r\n}\r\n#hahaha {\r\n    position: absolute;\r\n    padding: 5px;\r\n    transform: translate(-50%, -50%);\r\n    top: 20%;\r\n    left: 10%;\r\n    font-size: 20px;\r\n    color: #6D6D6D;\r\n    user-select: none;\r\n}\r\n.myAnimation {\r\n    position: fixed;\r\n    width: 60px;\r\n    height: 60px;\r\n    background-color: #0A1821;\r\n    border-radius: 100%;\r\n    z-index: 99;\r\n}\r\n.ghost {\r\n    position: fixed;\r\n    width: 80px;\r\n    height: 80px;\r\n    background-color: #33D1A0;\r\n    border-radius: 100%;\r\n    z-index: 98;\r\n    text-align: center;\r\n    line-height: 80px;\r\n    font-size: 40px;\r\n    -webkit-text-stroke: 1px #EEFFEE;\r\n    -webkit-text-fill-color: transparent;\r\n}\r\n\r\n\r\n\r\n/* .five-pointed-star,\r\n.five-pointed-star::before,\r\n.five-pointed-star::after{\r\n    width: 0;\r\n    height: 0;\r\n    border-left: var(--bw) solid transparent;\r\n    border-right: var(--bw) solid transparent;\r\n    border-top: calc(var(--bw) * 0.726542) solid #D91649; \r\n}\r\n\r\n.five-pointed-star{\r\n    --bw: 60px;\r\n    position: fixed;\r\n}\r\n.five-pointed-star::before{\r\n    content: '';\r\n    position: absolute;\r\n    transform: translate(-50%,-100%) rotate(70deg);\r\n}\r\n.five-pointed-star::after{\r\n    content: '';\r\n    position: absolute;\r\n    transform: translate(-50%,-100%) rotate(-70deg);\r\n} */\r\n\r\n/* .diamond{\r\nwidth: 50px;\r\nheight: 0;\r\nborder-style: solid;\r\nborder-color: transparent transparent red transparent;\r\nborder-width: 0 25px 25px 25px;\r\nposition: fixed;\r\nmargin: 20px 0 50px 0;\r\n}\r\n.diamond:after{\r\nwidth: 0;\r\nheight: 0;\r\ntop: 25px;\r\nleft: -25px;\r\nborder-style: solid;\r\nborder-color: red transparent transparent transparent;\r\nborder-width: 70px 50px 0 50px;\r\nposition: absolute;\r\ncontent: \"\";\r\n} */\r\n\r\n\r\n.square{\r\n    position: fixed;\r\n    width: 80px;\r\n    height: 80px;\r\n    background-color: #f5a631;\r\n    text-align: center;\r\n    line-height: 80px;\r\n    font-size: 40px;\r\n    -webkit-text-stroke: 1px #EEFFEE;\r\n    -webkit-text-fill-color: transparent;\r\n}\r\n\r\n\r\n</style>"],"sourceRoot":""}]}